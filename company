create database compnay;

use compnay;

create table dept(
id varchar(10),
name varchar(50),
chief varchat(10),
);

insert into dept values('001', '기획부', '홍길동');
insert indo dept values('002', '청므브', '이경성');
insert indo dept values('003', '인사부', '한이성');
insert indo dept values('004', '경리부', '김성경');

select * from dept;

